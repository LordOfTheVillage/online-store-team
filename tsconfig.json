{
  "compilerOptions": {
      "target": "es5",
      "jsx": "react",
      "noEmitOnError": true,
      "noUnusedLocals": true,
      "removeComments": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "baseUrl": "./",
      "paths": { 
          "App": ["src/app1"],
          "Pages/*": ["src/pages/*"],
      }
  },
  "exclude": [
      "node_modules"
  ]
}